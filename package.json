{
  "name": "@felipelealdefaria/experiments-service",
  "license": "ISC",
  "version": "0.1.1",
  "description": "Experiments service for creating A/B tests using lib sixpack-client",
  "author": "Lu√≠s Felipe Leal de Faria <felipelealdefaria@gmail.com> (https://github.com/felipelealdefaria)",
  "keywords": [
    "experiments",
    "typescript",
    "A/B tests",
    "sixpack-client",
    "clean architecture"
  ],
  "homepage": "https://github.com/felipelealdefaria/experiments-service#readme",
  "bugs": {
    "url": "https://github.com/felipelealdefaria/experiments-service/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "webpack-dev-server --open",
    "test:ci": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.2",
    "@types/axios": "^0.14.0",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.15.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "lint-staged": "^10.5.2",
    "node-sass": "^5.0.0",
    "path-browserify": "^1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^10.1.0",
    "slugify": "^1.4.6",
    "stream-http": "^3.1.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.13",
    "tsc-alias": "^1.2.6",
    "typescript": "^4.1.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "sixpack-client": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felipelealdefaria/experiments-service.git"
  }
}
